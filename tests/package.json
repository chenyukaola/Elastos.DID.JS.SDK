{
  "name": "@elastosfoundation/did-js-sdk-tests",
  "version": "0.0.0",
  "description": "Elastos DID Typescript SDK Tests",
  "scripts": {
    "dev": "rollup -c -w",
    "clean:tests": "rm -rf public/tests",
    "types": "npm run clean && tsc --emitDeclarationOnly --outDir typings/ --project tsconfig.types.json",
    "tsc:build": "rollup -c",
    "tsc:w": "tsc -w",
    "test:stubbrowserdata": "mkdir -p ./generated && echo '{}' > ./generated/browserdata.json",
    "test:node": "npm run test:stubbrowserdata && JEST_CIRCUS=1 jest --silent=true --config jest.node.config.ts",
    "test:node:debug": "JEST_CIRCUS=1 node --inspect-brk node_modules/.bin/jest --runInBand --config jest.node.config.ts",
    "test:browser": "npm run clean:tests && rollup -c rollup.browsertests.config.js"
  },
  "dependencies": {
    "@elastosfoundation/did-js-sdk": "^2.0.0",
    "buffer": "^5.6.0",
    "web3": "^1.3.6"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-inject": "^4.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.44",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "jest-lite": "^1.0.0-alpha.4",
    "path-browserify": "^1.0.1",
    "rollup": "^2.47.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-multi-input": "^1.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-polyfill-node": "^0.6.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "browserslist": "> 0.25%, not dead",
  "author": "Elastos foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elastos/Elastos.DID.JS.SDK/issues"
  },
  "homepage": "https://github.com/elastos/Elastos.DID.JS.SDK#readme"
}
